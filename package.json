{
  "name": "discord-game-bot",
  "version": "1.0.0",
  "description": "Discord bot for games",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "postbuild": "prisma generate",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev --name",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "db:setup": "npm run prisma:generate && npm run prisma:push",
    "db:migrate": "npm run prisma:generate && npm run prisma:migrate",
    "db:reset": "npm run prisma:reset",
    "railway:start": "prisma generate && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
